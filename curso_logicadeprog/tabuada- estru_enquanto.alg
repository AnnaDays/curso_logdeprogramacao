algoritmo "tabuada  - enquanto"
// Nome: Ana Luiza dos Santos Dias
var
num, cont, resultado: inteiro
inicio
// Seção de Comandos
cont <- 0
     escreva("Digite um número: ")
     leia(num)
     
     enquanto (cont < 10) faca
     cont <- cont + 1
     resultado <- num * cont
     escreval(num, " X", cont, " = ",resultado)
     fimenquanto
fimalgoritmo

